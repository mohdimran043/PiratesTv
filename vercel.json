{
  "functions": {
    "api/filter.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/filter",
      "dest": "/api/filter.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}